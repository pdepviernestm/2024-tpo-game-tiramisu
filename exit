[32m+[m[32mimport base.*[m
[32m+[m
[32m+[m[32mobject mario {[m
[32m+[m[32m  var property position = game.at(0,2)[m
[32m+[m[32m  var property saltando = false[m[41m [m
[32m+[m[32m  var property andar = false[m
[32m+[m[32m  var property mirarDer = true[m
[32m+[m[32m  var imagen = "marioD3.png"[m
[32m+[m
[32m+[m[32m  method image() = imagen[m
[32m+[m[41m  [m
[32m+[m[32m  method position(newPos) {[m
[32m+[m[32m    position = newPos[m
[32m+[m[32m    }[m
[32m+[m[41m  [m
[32m+[m[32m  method derecha() {[m
[32m+[m[32m    if (self.dentroDePantalla(self.position().right(1))){[m
[32m+[m[32m      if (andar){[m
[32m+[m[32m      imagen = "marioD3.png"[m
[32m+[m[32m      andar = false[m
[32m+[m[32m      }[m
[32m+[m[32m    else {[m
[32m+[m[32m      imagen = "marioDD3.png"[m
[32m+[m[32m      andar = true[m
[32m+[m[32m      }[m
[32m+[m[32m    mirarDer = true[m
[32m+[m[32m    self.position(self.position().right(1))[m[41m [m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m[32m  method izquierda() {[m
[32m+[m[32m    if (self.dentroDePantalla(self.position().left(1))){[m
[32m+[m[32m    if (andar){[m
[32m+[m[32m      imagen = "marioI3.png"[m
[32m+[m[32m      andar = false[m
[32m+[m[32m      }[m
[32m+[m[32m    else {[m
[32m+[m[32m      imagen = "marioII3.png"[m
[32m+[m[32m      andar = true[m
[32m+[m[32m      }[m
[32m+[m[32m    mirarDer = false[m
[32m+[m[32m    self.position(self.position().left(1))[m[41m [m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m[32m  method dentroDePantalla(pos) = ((pos.x() > -1) and (pos.x() < game.width()))[m
[32m+[m
[32m+[m[32m  method saltar() {[m
[32m+[m[32m    if (not(saltando)) {[m
[32m+[m[32m      var imagenOld[m
[32m+[m[32m      self.saltando(true)[m[41m [m
[32m+[m[32m      andar = false[m
[32m+[m
[32m+[m[32m      if(mirarDer) {[m
[32m+[m[32m        imagen= "marioUpD3.png"[m
[32m+[m[32m        imagenOld = "marioD3.png"[m
[32m+[m[32m        }[m[41m [m
[32m+[m[41m    [m
[32m+[m[32m      else{[m[41m [m
[32m+[m[32m        imagen= "marioUpI3.png"[m
[32m+[m[32m        imagenOld = "marioI3.png"[m
[32m+[m[32m        }[m
[32m+[m[41m    [m
[32m+[m[32m    self.position(self.position().up(1))[m
[32m+[m[32m    game.schedule(1000, {[m[41m [m
[32m+[m[32m      self.position(self.position().down(1))[m[41m [m
[32m+[m[32m      game.schedule(20,  {imagen = imagenOld})[m[41m [m
[32m+[m[32m    self.saltando(false) })[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  method enBase() = game.colliders(self).any({ elem => elem.soyBase()})[m
[32m+[m
[32m+[m[32m  method caer() {[m
[32m+[m[32m    if (self.dentroDePantalla(self.position().down(1))){[m
[32m+[m[32m      self.position(self.position().down(1))[m
[32m+[m[32m    }[m
[32m+[m[32m    else self.endGame()[m
[32m+[m[32m  }[m
[32m+[m[41m  [m
[32m+[m[32m  method endGame() {[m
[32m+[m[32m      if (mirarDer) imagen = "marioDeadD.png"[m
[32m+[m[32m      else imagen = "marioDeadI.png"[m
[32m+[m
[32m+[m[32m      game.schedule(1000, { game.stop() })[m
[32m+[m[32m  }[m
[32m+[m[32m}[m

[32m+[m[32mprogram DonkeyKongGame {[m
[32m+[m	[32mgame.title("Donkey Kong")[m
[32m+[m	[32mgame.height(15)[m
[32m+[m	[32mgame.width(15)[m
[32m+[m	[32mgame.cellSize(40)[m
[32m+[m	[32mgame.boardGround("blackBackground.jpg")[m
[32m+[m
[32m+[m	[32mgame.showAttributes(mario)[m
[32m+[m	[32mgame.addVisual(mario)[m
[32m+[m
[32m+[m
[32m+[m	[32mconst teclasDer = [keyboard.right(),keyboard.d()][m
[32m+[m	[32mteclasDer.forEach { n =>  n.onPressDo({ mario.derecha() })}[m
[32m+[m
[32m+[m	[32mconst teclasIzq = [keyboard.left(),keyboard.a()][m
[32m+[m	[32mteclasIzq.forEach { n =>  n.onPressDo({ mario.izquierda() })}[m
[32m+[m
[32m+[m	[32mconst teclasSaltar = [keyboard.up(),keyboard.space(),keyboard.w()][m
[32m+[m	[32mteclasSaltar.forEach { n =>  n.onPressDo({ mario.saltar() })}[m
[32m+[m
